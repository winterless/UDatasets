{
  "dataset": {
    "name": "Toucan-1.5M-oss+sft"
  },
  "sources": [
    {
      "type": "parquet",
      "data_dir": "Toucan-1.5M/Toucan-1.5M",
      "globs": [
        "OSS/**/*.parquet",
        "SFT/**/*.parquet"
      ]
    }
  ],
  "adapter": {
    "kind": "toucan"
  },
  "executor": {
    "target_shard_mb": 50,
    "min_shard_mb": 2,
    "token_billions": 0.0
  }
}
