{
  "enabled": false,
  "dataset": {
    "name": "Nemotron-Pretraining-SFT-v1"
  },
  "sources": [
    {
      "type": "parquet",
      "data_dir": "Nemotron-Pretraining-SFT-v1",
      "glob": "**/*.parquet"
    }
  ],
  "adapter": {
    "kind": "nemotron-pretraining-sft-v1"
  },
  "executor": {
    "start_method": "fork",
    "target_shard_mb": 50,
    "token_billions": 0.01,
    "tasks": 192,
    "workers": 192,
    "merge_to_files": 1
  }
}

