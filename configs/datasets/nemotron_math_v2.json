{
  "enabled": true,
  "dataset": {
    "name": "Nemotron-Math-v2"
  },
  "sources": [
    {
      "type": "jsonl",
      "data_dir": "Nemotron-Math-v2/data",
      "glob": "**/*.jsonl",
      "path_keywords": ["high"]
    }
  ],
  "adapter": {
    "kind": "nemotron-math-v2"
  },
  "executor": {
    "start_method": "fork",
    "token_budget_mode": "global_pool",
    "target_shard_mb": 50000000,
    "min_shard_mb": 2,
    "token_billions": 40,
    "tasks": 192,
    "workers": 192,
    "merge_to_files": 1
  }
}

