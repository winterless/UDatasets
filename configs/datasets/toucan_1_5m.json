{
  "dataset": {
    "name": "Toucan-1.5M"
  },
  "sources": [
    {
      "type": "parquet",
      "data_dir": "Toucan-1.5M/Toucan-1.5M/",
      "glob": "**/*.parquet"
    }
  ],
  "adapter": {
    "kind": "toucan"
  },
  "executor": {
    "target_shard_mb": 0.5,
    "token_billions": 0.001
  },
  "postprocess": {
    "kinds": ["param_pool", "tool_mcq"],
    "output_subdir": "prepare_aug",
    "max_mcq": 1,
    "pretty_txt": true
  },
  "enabled": true
}
