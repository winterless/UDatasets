{
  "dataset": {
    "name": "Toucan-1.5M"
  },
  "sources": [
    {
      "type": "parquet",
      "data_dir": "Toucan-1.5M/Toucan-1.5M/",
      "glob": "**/*.parquet"
    }
  ],
  "adapter": {
    "kind": "toucan"
  },
  "executor": {
    "target_shard_mb": 5
  },
  "postprocess": {
    "kinds": ["param_pool", "tool_mcq"],
    "output_subdir": "prepare_aug",
    "mcq_every_n": 5,
    "pretty_txt": true
  },
  "enabled": true
}
